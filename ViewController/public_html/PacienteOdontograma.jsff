                            function cerrarPopup(actionEvent){
                              var source = actionEvent.getSource();
                              var registrar = source.findComponent("popupRegistrarProcedimientoPresupuesto");
                              var editar = source.findComponent("popupEditarProcedimientoPresupuesto");
                              var nuevo = source.findComponent("popupNuevoPresupuesto");
                              var eliminar = source.findComponent("popupEliminarPresupuesto");
                              
                                if (registrar.isPopupVisible()) {
                                  registrar.hide();
                                }
                                if (editar.isPopupVisible()) {
