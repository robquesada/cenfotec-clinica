                                mensaje.setValue('');
                                
                                if(procedimiento.getSubmittedValue()==''){
                                  mensaje.setValue('Debe seleccionar datos en los campos señalados');
                                  procedimiento.setChanged(true);
                                  actionEvent.cancel();
